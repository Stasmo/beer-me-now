        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <style type="text/css">
        /* Small devices (tablets, 768px and up) */
        @media (min-width: 768px) {
          .searchTerm {
            height: 3em;
            border-radius: 2em;
            line-height: 2em;
            font-size: 2em;
          }
         }

        /* Medium devices (desktops, 992px and up) */
        @media (min-width: 992px) {  }

        /* Large devices (large desktops, 1200px and up) */
        @media (min-width: 1200px) { 
          .searchTerm {
            line-height: 2em;
            font-size: 2em;
          }
        }

          html { height: 100% }
          body { height: 100%; margin: 0; padding: 0 }
          #map { height: 100%; background-color: #777; }

          .search {
              width: 100%;
              position: relative;
              padding: 0.5em;
              background: none;
          }
          .searchTerm {
            outline: none;
            width: 100%;
            position: relative;
            height: 2em;
            padding-left: 1em;
            border-radius: 1em;
            border: 3px solid #298;
            background-color: #fff;
          }

        </style>
        <link rel="stylesheet" href="bundles/stasmoexamplesampleapi/css/bootstrap-theme.min.css">
        <link rel="stylesheet" href="bundles/stasmoexamplesampleapi/css/main.css">

